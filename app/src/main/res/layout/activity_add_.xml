        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.42"
        app:srcCompat="@drawable/ic_action_dolar" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="28dp"
