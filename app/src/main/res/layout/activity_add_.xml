    <EditText
        android:id="@+id/editText5"
        android:layout_width="256dp"
        android:layout_height="47dp"
        android:ems="10"
        android:fontFamily="@font/biryani_semibold"
        android:hint="@string/metodo"
        android:inputType="textPersonName"
        android:paddingHorizontal="15dp"
        android:textColor="@color/vino"
        android:textColorHint="@color/vino"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.826" />

    <Button
        android:id="@+id/bfecha"
        style="@style/Widget.AppCompat.Button.Borderless"
        android:layout_width="51dp"
        android:layout_height="40dp"
        android:background="@drawable/boton_fecha"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.872"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.542" />

    <EditText
        android:id="@+id/efecha"
        android:layout_width="150dp"
        android:layout_height="43dp"
        android:ems="10"
        android:fontFamily="@font/biryani_semibold"
        android:hint="Fecha de pago"
        android:inputType="textPersonName"
        android:paddingHorizontal="15dp"
        android:textColor="@color/vino"
        android:textColorHint="@color/vino"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.398"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.543" />

    <Button
        android:id="@+id/buttonAdd"
        android:layout_width="366dp"
        android:layout_height="35dp"
